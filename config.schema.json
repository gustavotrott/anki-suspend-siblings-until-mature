{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "required": [
        "enabled_for_decks",
        "quiet",
        "unsuspend_on",
        "version"
    ],
    "properties": {
        "enabled_for_decks": {
            "type": "object",
            "patternProperties": {
                "^\\d+$": {
                    "type": "boolean"
                }
            },
            "additionalProperties": false
        },
        "quiet": {
            "type": "boolean"
        },
        "unsuspend_on": {
            "type": "integer"
        },
        "version": {
            "const": 0
        }

    }
}
